{
  "name": "minisearch",
  "version": "6.1.0",
  "description": "Tiny but powerful full-text search engine for browser and Node",
  "author": "Luca Ongaro",
  "homepage": "https://lucaong.github.io/minisearch/",
  "bugs": "https://github.com/lucaong/minisearch/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucaong/minisearch.git"
  },
  "keywords": [
    "search",
    "full text",
    "fuzzy",
    "prefix",
    "auto suggest",
    "auto complete",
    "index"
  ],
  "license": "MIT",
  "main": "dist/umd/index.js",
  "module": "dist/es5m/index.js",
  "es2015": "dist/es/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/es/index.js"
    },
    "./SearchableMap": {
      "types": "./dist/types/SearchableMap.d.ts",
      "require": "./dist/cjs/SearchableMap.cjs",
      "default": "./dist/es/SearchableMap.js"
    }
  },
  "unpkg": "./dist/umd/index.js",
  "jsdelivr": "./dist/umd/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "/dist/**/*",
    "/src/**/*"
  ],
  "scripts": {
    "benchmark": "pnpm build-benchmark && NODE_ENV=production node --expose-gc benchmarks/dist/index.cjs",
    "build": "pnpm clean && NODE_ENV=production rollup -c",
    "build-benchmark": "BENCHMARKS=true pnpm build",
    "build-minified": "MINIFY=true pnpm build",
    "build-docs": "typedoc --options typedoc.json && touch docs/.nojekyll && pnpm build-examples",
    "build-examples": "pnpm build && cd examples && pnpm install && pnpm build && cd ../",
    "coverage": "vitest --coverage",
    "clean": "rm -rf dist",
    "test": "vitest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lintfix": "eslint --fix 'src/**/*.{js,ts}'",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "sideEffects": false,
  "dependencies": {
    "vitest": "^0.31.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/benchmark": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "benchmark": "^2.1.4",
    "core-js": "^3.30.2",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fast-check": "^3.9.0",
    "regenerator-runtime": "^0.13.11",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "snazzy": "^9.0.0",
    "tslib": "^2.5.2",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  }
}
