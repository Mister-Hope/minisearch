{
  "name": "slimsearch",
  "version": "0.0.1",
  "description": "Tiny but powerful full-text search engine for browser and Node",
  "type": "module",
  "author": "Luca Ongaro, Mister Hope",
  "homepage": "https://mister-hope.github.io/slimsearch/",
  "bugs": "https://github.com/Mister-Hope/slimsearch/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mister-Hope/slimsearch.git"
  },
  "keywords": [
    "search",
    "full text",
    "fuzzy",
    "prefix",
    "auto suggest",
    "auto complete",
    "index"
  ],
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./SearchableMap": {
      "types": "./dist/SearchableMap.d.ts",
      "require": "./dist/SearchableMap.cjs",
      "default": "./dist/SearchableMap.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "benchmark": "pnpm build-benchmark && NODE_ENV=production node --expose-gc benchmarks/dist/index.cjs",
    "build": "rollup -c rollup.config.ts --config-plugin esbuild",
    "build-benchmark": "BENCHMARKS=true pnpm build",
    "build-minified": "MINIFY=true pnpm build",
    "docs:build": "typedoc --options typedoc.json",
    "coverage": "vitest --coverage",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.ts",
    "prepublishOnly": "pnpm test && pnpm clean && pnpm build",
    "test": "vitest"
  },
  "sideEffects": false,
  "dependencies": {
    "vitest": "^0.31.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@types/benchmark": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "benchmark": "^2.1.4",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-check": "^3.9.0",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.2",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  }
}
